local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("SS's Universal GUI", "Ocean")

local Tab = Window:NewTab("Admin Scripts")

local Section = Tab:NewSection("Admin Scripts")

Section:NewButton("Infinite Yield", "Loads IY", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'), true))()
end)

Section:NewButton("Fates Admin", "Loads FA", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/udAZFtdk'), true))()
end)

Section:NewButton("CMD-X", "Loads CX", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/CSE3Uxmk'), true))()
end)

local Tab = Window:NewTab("ESP")

local Section = Tab:NewSection("ESP")

local esp_enabled = false;
Section:NewToggle("ESP (Highlight)", "ToggleInfo", function(state)
    esp_enabled = state;
    while esp_enabled do
        task.wait()
        for i,v in next, game.Players:GetPlayers() do
            if v.Name ~= game.Players.LocalPlayer.Name and not v.Character:FindFirstChild("Highlight") then
                if v ~= nil and v.Character ~= nil and game.Workspace:FindFirstChild(v.Name) then
                    local highlight = Instance.new("Highlight");
                    highlight.Parent = v.Character;
                end
            end
        end
    end
    Instance.delete("highlight")
end)

